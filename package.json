  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TimeMagazineLabs/d3axis/issues"
  },
  "homepage": "https://github.com/TimeMagazineLabs/d3axis#readme",
  "dependencies": {
    "d3-selection": "^1.4.2",
    "d3-transition": "^1.3.2"
  }
}
